names(data)


data[,3:4] #Columns 3:4
data[15:20,] #Rows 15:20
data[1,4] # Row 1, column 4
describe(data$Warn)

data2<-data[,-17] # Drops column 17
dim(data)
dim(data2)

data2<-data2[-10,] # Drops row 10
dim(data)
dim(data2)
data3<-data[-10,]

data$Con<-apply(data[7:16],1,sum)
describe(data$Con)

